      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "link-module-alias": "^1.2.0",
    "prettier": "^2.1.1"
  },
  "_moduleAliases": {
    "app": "src/",
    "assets": "src/assets",
